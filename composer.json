{
    "name": "free-elephants/thruway-in-docker",
    "type": "project",
    "require": {
        "voryx/thruway": "0.6.1",
        "firebase/php-jwt": "6.*",
        "free-elephants/di": "^4.0"
    },
    "authors": [
        {
            "name": "samizdam",
            "email": "samizdam@inbox.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "FreeElephants\\Thruway\\": "src/FreeElephants/Thruway/",
            "Ratchet\\Server\\": "src/Ratchet/Server/",
            "React\\Socket\\": "src/React/Socket/",
            "Thruway\\Transport\\": "src/Thruway/Transport/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "FreeElephants\\Thruway\\": "tests/FreeElephants/Thruway/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "thruway/pawl-transport": "^0.5.2"
    },
    "config": {
        "sort-packages": true
    }
}
